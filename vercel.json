{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/hackrx.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "bajaj_V3/frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/hackrx/run",
      "dest": "api/hackrx.py"
    },
    {
      "src": "/hackrx/(.*)",
      "dest": "api/hackrx.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/upload",
      "dest": "api/index.py"
    },
    {
      "src": "/query",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    },
    {
      "src": "/frontend/(.*)",
      "dest": "bajaj_V3/frontend/$1"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "runtime": "python3.9"
    },
    "api/hackrx.py": {
      "maxDuration": 60,
      "runtime": "python3.9"
    }
  }
}
